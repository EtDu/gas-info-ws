{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": "current"
        },
        "modules": false
      }
    ]
  ],
  "plugins": [
    [
      "transform-rename-import",
      {
        "original": "^((?:\\.\\.|\\.)\\/[^\\.]+?)$",
        "replacement": "$1.js"
      }
    ]
  ]
}